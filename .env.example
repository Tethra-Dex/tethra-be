PORT=3001
NODE_ENV=development

DEBUG=true

# Oracle provider: "chainlink" (default) or "pyth"
ORACLE_PROVIDER=chainlink

PRICE_SIGNER_PRIVATE_KEY=
RELAY_PRIVATE_KEY=

USDC_TOKEN_ADDRESS=
TETHRA_TOKEN_ADDRESS=
VAULT_POOL_ADDRESS=
STABILITY_FUND_ADDRESS=

# Optional: override Stability Fund streamer interval (default 6 hours)
VAULT_STREAM_INTERVAL_MINUTES=360

MARKET_EXECUTOR_ADDRESS=
POSITION_MANAGER_ADDRESS=
RISK_MANAGER_ADDRESS=
LIMIT_EXECUTOR_ADDRESS=
TAP_TO_TRADE_EXECUTOR_ADDRESS=
ONE_TAP_PROFIT_ADDRESS=

TETHRA_STAKING_ADDRESS=

USDC_PAYMASTER_ADDRESS=

DEPLOYER_ADDRESS=
PRICE_SIGNER_ADDRESS=
KEEPER_ADDRESS=
TEAM_ADDRESS=
PROTOCOL_TREASURY_ADDRESS=

CHAIN_ID=84532
NETWORK_NAME=base-sepolia
RPC_URL=https://sepolia.base.org

# Chainlink price feed configuration
# Optional: use a different RPC for Chainlink reads than trading/contract RPC.
# Useful if contracts are on Base Sepolia but you want to read Base mainnet feeds.
# CHAINLINK_RPC_URL=https://mainnet.base.org
#
# Option A: Provide each feed individually (recommended)
# CHAINLINK_FEED_BTC=0x...
# CHAINLINK_FEED_ETH=0x...
# CHAINLINK_FEED_XAUUSD=0x...
# CHAINLINK_FEED_USOIL=0x...
# CHAINLINK_FEED_EURUSD=0x...
# CHAINLINK_FEED_GBPUSD=0x...
# CHAINLINK_FEED_US30=0x...
# CHAINLINK_FEED_NASDAQ=0x...
#
# Option B: Provide all feeds as a JSON object (SYMBOL -> address)
# CHAINLINK_FEEDS={"BTC":"0x...","ETH":"0x...","XAUUSD":"0x..."}
#
# Polling settings
CHAINLINK_POLL_INTERVAL_MS=1200
# NOTE: Chainlink feeds may only update on heartbeat (minutes/hours) or deviation thresholds.
# Use a higher max age than Pyth/WebSocket feeds.
CHAINLINK_MAX_PRICE_AGE_SEC=90
